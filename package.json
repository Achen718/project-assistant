{
  "name": "ai-project-assistant",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "module": "./dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js",
      "default": "./dist/client.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "build-lib": "tsup package/index.ts package/client.ts --format cjs,esm --dts --tsconfig tsconfig.build.json --external react,react-dom --splitting --clean",
    "prepublishOnly": "npm run build-lib",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "type-check:src": "tsc --noEmit --skipLibCheck --skipDefaultLibCheck --project tsconfig.src.json",
    "test": "jest",
    "test:direct": "node direct-integration-test.js"
  },
  "dependencies": {
    "@ai-sdk/groq": "^1.2.9",
    "@ai-sdk/openai": "^1.3.22",
    "@ai-sdk/react": "^1.2.12",
    "@heroicons/react": "^2.2.0",
    "@langchain/community": "^0.3.42",
    "@langchain/core": "^0.3.55",
    "@langchain/langgraph": "^0.2.71",
    "@langchain/openai": "^0.5.10",
    "@radix-ui/react-slot": "^1.2.2",
    "@supabase/supabase-js": "^2.49.5",
    "ai": "^4.3.15",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "^11.7.1",
    "firebase-admin": "^12.7.0",
    "lucide-react": "^0.509.0",
    "next": "15.3.2",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^3.2.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.4",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.3.2",
    "tsup": "^8.5.0",
    "tw-animate-css": "^1.2.9",
    "typescript": "^5"
  }
}
